<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="inherited_view_invoice_supplier_form" model="ir.ui.view">
            <field name="name">inherited_view_invoice_supplier_form</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="account.invoice_supplier_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='reconciled']" position="after">
                    <field name="edi_vendor_bill_total_untaxed"/>
                    <field name="edi_vendor_bill_total"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>